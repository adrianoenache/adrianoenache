{
  "name": "setup-webpack-stylus-liquid",
  "version": "0.0.1",
  "description": "setup-webpack-stylus-liquid",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm run cleanDistFolder && webpack-dev-server --open",
    "build": "webpack",
    "prod": "NODE_ENV=production && npm run cleanDistFolder && webpack",
    "start": "npm run dev && webpack-dev-server",
    "setup": "npm i && npm run start",
    "cleanDistFolder": "rimraf ./dist/*",
    "removeNode": "rm -rf node_modules && npm cache clean -f",
    "uninstall": "npm run cleanDistFolder && npm run removeNode",
    "reinstall": "npm run cleanDistFolder && npm run removeNode && npm run setup"
  },
  "author": "Adriano Enache",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.3.7",
    "css-loader": "^6.4.0",
    "cssnano": "^5.0.8",
    "cssnano-preset-advanced": "^5.1.4",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "image-minimizer-webpack-plugin": "^2.2.0",
    "imagemin-svgo": "^10.0.0",
    "liquid": "^5.1.1",
    "liquid-loader": "^0.1.4",
    "mini-css-extract-plugin": "^2.4.3",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.0",
    "purgecss": "^4.0.3",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.2.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
