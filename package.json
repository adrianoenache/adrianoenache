{
  "name": "setup-webpack-stylus-liquid",
  "version": "0.0.1",
  "description": "setup-webpack-stylus-liquid",
  "main": "webpack.config.js",
  "scripts": {
    "dev": "npm run clean && webpack -d",
    "prod": "NODE_ENV=production && npm run clean && webpack -p",
    "start": "npm run dev && webpack-dev-server",
    "setup": "npm i && npm run start",
    "clean": "rimraf ./dist/*",
    "uninstall": "npm run clean && rm -rf node_modules && npm cache clean -f",
    "reinstall": "npm run clean && rm -rf node_modules && npm cache clean -f && npm run setup"
  },
  "author": "Adriano Enache",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "css-loader": "^5.2.0",
    "cssnano-preset-advanced": "^4.0.7",
    "cssnano": "^4.1.10",
    "file-loader": "^6.2.0",
    "glob": "^7.1.6",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "image-webpack-loader": "^7.0.1",
    "liquid-loader": "^0.1.4",
    "liquid": "^5.1.1",
    "mini-css-extract-plugin": "^1.4.0",
    "path": "^0.12.7",
    "postcss-loader": "^5.2.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "stylus-loader": "^5.0.0",
    "stylus": "^0.54.8",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack": "^5.28.0"
  }
}
