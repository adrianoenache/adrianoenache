{
  "name": "setup-webpack-stylus-liquid",
  "version": "1.0.0",
  "description": "setup-webpack-stylus-liquid",
  "main": "webpack.config.js",
  "scripts": {
    "install": "npm i",
    "startDev": "npm run dev && npm run openServerDev",
    "startProd": "npm run prod && npm run openServerProd",
    "cleanDistFolder": "rimraf ./dist/*",
    "cleanProdFolder": "rimraf ./prod/*",
    "removeNode": "rm -rf node_modules && npm cache clean -f",
    "uninstall": "npm run cleanDistFolder && npm run cleanProdFolder && npm run removeNode",
    "reinstall": "npm run cleanDistFolder && npm run cleanProdFolder && npm run removeNode && npm run install",
    "dev": "npm run cleanDistFolder && npm run buildDev",
    "prod": "npm run cleanProdFolder && npm run buildProd",
    "buildDev": "webpack --node-env=development",
    "buildProd": "webpack --node-env=production",
    "openServerDev": "webpack-dev-server --node-env=development --open",
    "openServerProd": "webpack-dev-server --node-env=production --open"
  },
  "author": "Adriano Enache",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.4.0",
    "cssnano": "^5.0.8",
    "cssnano-preset-advanced": "^5.1.4",
    "file-loader": "^6.2.0",
    "glob": "^7.2.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "liquidjs": "^9.28.3",
    "liquidjs-loader": "^1.0.1",
    "mini-css-extract-plugin": "^2.4.3",
    "mini-svg-data-uri": "^1.4.3",
    "path": "^0.12.7",
    "postcss-loader": "^6.2.0",
    "purgecss": "^4.0.3",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.2.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}
